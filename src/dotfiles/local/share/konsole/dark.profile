[Appearance]
BoldIntense=true
ColorScheme=AdaptaNokto
Font=FiraCode NF,14,-1,5,53,0,0,0,0,0,Retina

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/zsh
Environment=TERM=konsole-256color,TERM_META=dark
LocalTabTitleFormat=%w - %d : %n
Name=dark
Parent=FALLBACK/
ShowTerminalSizeHint=false

[Scrolling]
HistoryMode=1
HistorySize=1000000
ScrollBarPosition=2
ScrollFullPage=true
