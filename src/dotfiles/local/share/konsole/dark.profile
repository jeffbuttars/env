[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Sweet-Mars
DimmValue=35
Font=FiraCode Nerd Font Mono,14,-1,5,50,0,0,0,0,0
LineSpacing=0
UseFontLineChararacters=true

[Cursor Options]
CursorShape=1
CustomCursorColor=255,255,255
UseCustomCursorColor=false

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/zsh
DimWhenInactive=true
Environment=TERM=konsole-256color,TERM_META=dark
InvertSelectionColors=true
LocalTabTitleFormat=%w - %d : %n
Name=dark
Parent=FALLBACK/
SemanticInputClick=false
SemanticUpDown=false
ShowTerminalSizeHint=true
TerminalCenter=true
TerminalColumns=100
TerminalMargin=1
TerminalRows=64

[Interaction Options]
TrimLeadingSpacesInSelectedText=true
TrimTrailingSpacesInSelectedText=true

[Scrolling]
HistoryMode=1
HistorySize=1000000
ScrollBarPosition=2
ScrollFullPage=true

[Terminal Features]
BellMode=3
BlinkingCursorEnabled=true
VerticalLine=false
VerticalLineAtChar=99
