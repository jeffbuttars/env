[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Solarized Light
Font=MesloLGM Nerd Font Mono,14,-1,5,50,0,0,0,0,0,RegularForPowerline

[Cursor Options]
CursorShape=0
CustomCursorColor=255,0,0
UseCustomCursorColor=true

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/zsh
Environment=TERM=konsole-256color,TERM_META=light
Icon=utilities-terminal
Name=Light
Parent=FALLBACK/

[Interaction Options]
AutoCopySelectedText=true
TrimTrailingSpacesInSelectedText=true

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=1
HistorySize=1000000
ScrollBarPosition=1

[Terminal Features]
BidiRenderingEnabled=false
BlinkingTextEnabled=true
FlowControlEnabled=false
