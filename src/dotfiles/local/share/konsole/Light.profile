[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=papercolor-light
Font=FiraCode Nerd Font Mono,14,-1,5,53,0,0,0,0,0,Retina

[Cursor Options]
CursorShape=0
CustomCursorColor=255,0,0
UseCustomCursorColor=true

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/zsh
Environment=TERM=konsole-256color,TERM_META=light,BAT_THEME=ansi-light
Icon=utilities-terminal
Name=Light
Parent=FALLBACK/

[Interaction Options]
AutoCopySelectedText=true
TrimTrailingSpacesInSelectedText=true

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=1
HistorySize=1000000
ScrollBarPosition=1

[Terminal Features]
BidiRenderingEnabled=false
BlinkingTextEnabled=true
FlowControlEnabled=false
