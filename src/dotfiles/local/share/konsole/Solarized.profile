[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Solarized
Font=Fira Mono,14,-1,5,50,0,0,0,0,0
LineSpacing=0
UseFontLineChararacters=false

[Cursor Options]
CursorShape=1

[General]
Command=/bin/zsh
DimWhenInactive=false
Environment=TERM=xterm-256color,COLORTERM=truecolor,TERM_META=dark
InvertSelectionColors=false
Name=Solarized
Parent=FALLBACK/
ShowTerminalSizeHint=true
TerminalCenter=false
TerminalMargin=1

[Interaction Options]
AutoCopySelectedText=true
CopyTextAsHTML=false
TrimLeadingSpacesInSelectedText=true
TrimTrailingSpacesInSelectedText=true

[Keyboard]
KeyBindings=default

[Scrolling]
HighlightScrolledLines=true
HistorySize=100000

[Terminal Features]
BellMode=3
BidiRenderingEnabled=true
BlinkingCursorEnabled=true
